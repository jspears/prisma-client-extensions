generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["clientExtensions", "extendedWhereUnique"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  companyId String @default(cuid())
  id        String @default(cuid())
  firstName String
  lastName  String
  email     String

  @@id([companyId, id])
}
